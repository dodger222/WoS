@model WorkplaceOfSecretary.Models.Meeting

@{
    ViewBag.Title = "Details";
}

<h2>Информация о заседании</h2>

<div>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayName("ГЭК:")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Seb.NameOfSEB)
        </dd>

        <dt>
            @Html.DisplayName("Дата:")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Date)
        </dd>

    </dl>
</div>
<p>
    @Html.ActionLink("К списку заседаний", "Index", "Meeting", null, new { @class = "btn btn-default btn-radius" })
    @Html.ActionLink("Редактировать", "Edit", "Meeting", new { id = Model.ID }, new { @class = "btn btn-default btn-radius" })
</p>
